{
  "compilerOptions": {
    "target": "es5",
    "downlevelIteration": true,
    "typeRoots": [
        "./types",
        "./node_modules/@types"
    ],
    "lib": [
      "dom",
      "es2017"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "incremental": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "pages/home.tsx"  ],
  "exclude": [
    "node_modules",
  ]
}
